pipeline {
    agent any
    stages {
        
        stage('build') {
            steps {
                withPythonEnv('python') {
                    sh 'python --version'
                }
            }
        }
        stage('Deploy'){
            steps{
                withPythonEnv('python') {
                    sh '/var/jenkins_home/workspace/poopie_discordbot_max-dev/poopiBot-Max.py'
                }
            }
        }
    }
}