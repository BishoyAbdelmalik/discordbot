pipeline {
    agent any
    stages {
        
        stage('build') {
            steps {
                withPythonEnv('python3') {
                    sh 'python --version'
                }
            }
        }
        stage('Deploy'){
            steps{
                withPythonEnv('python3') {
                    sh 'python poopiBot-Max.py'
                }
            }
        }
    }
}